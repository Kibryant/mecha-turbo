{
  "expo": {
    "name": "DNA da Mecha Turbo",
    "slug": "dna-mecha-turbo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/logotipo-mecha-turbo-vertical-icon.png",
    "scheme": "dna-mecha-turbo",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./public/logotipo-mecha-turbo-vertical-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D0D0D"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/logotipo-mecha-turbo-vertical-adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./public/logotipo-mecha-turbo-vertical-icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-localization",
      "expo-font",
      "./app.plugin.js"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}