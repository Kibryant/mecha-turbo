{
  "expo": {
    "name": "mecha-turbo",
    "slug": "dna-mecha-turbo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo/logotipo-mecha-turbo-vertical.png",
    "scheme": "dna-mecha-turbo",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo/logotipo-mecha-turbo-vertical.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D0D0D"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dna.mecha.turbo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo/logotipo-mecha-turbo-vertical.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dna.mecha.turbo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo/logotipo-mecha-turbo-vertical.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
